<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ezlinker.app.modules.feature.mapper.FeatureMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ezlinker.app.modules.feature.model.Feature">
    <result column="id" property="id" />
    <result column="record_version" property="recordVersion" />
    <result column="x" property="x" />
    <result column="create_time" property="createTime" />
        <result column="label" property="label" />
        <result column="name" property="name" />
        <result column="type" property="type" />
        <result column="product_id" property="productId" />
        <result column="cmd_key" property="cmdKey" />
        <result column="cmd_value" property="cmdValue" typeHandler="com.baomidou.mybatisplus.extension.handlers.JacksonTypeHandler"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        record_version,
        x,
        create_time,
        label, name, type, product_id, cmd_key, cmd_value
    </sql>

</mapper>
